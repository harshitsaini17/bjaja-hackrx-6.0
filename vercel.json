{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/main.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.9",
    "APP_MODULE": "api.main:app"
  },
  "functions": {
    "api/main.py": {
      "maxDuration": 60
    }
  }
}
